<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowResultActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/go_manage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="继续" />

        <TextView
            android:id="@+id/pressure_level_resultTxv"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:text="TextView"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/num_of_danger_resultTxv"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:text="TextView"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/tod_resultTxv"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="TextView"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/diabetes_resultTxv"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:text="TextView"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/ACC_resultTxv"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:text="TextView"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/danger_levelTxv"
            android:layout_width="match_parent"
            android:layout_height="208dp"
            android:text="TextView"
            android:textSize="24sp" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>